<div class="l-flex klubs-page">

  <div class="l-flex l-flex--vert map-and-klubpage">

    {{leaflet-map wantedZoom=zoom wantedCenter=markerCenter klubs=model showKlub="showKlub" isShowPage=isShowPage categoryShown=category}}

    {{outlet}}
  </div>

  {{#unless anyKlub}}
    <div class="loader loader--embeds">
      <div class="spinner-container">
        <div class="spinner"></div>
        <div class="spinner"></div>
        <div class="spinner"></div>
      </div>
    </div>
  {{/unless}}

</div>